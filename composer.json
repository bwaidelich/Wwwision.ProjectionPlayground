{
    "description": "Neos backend module that allows for creating and testing ESCR projections on the fly",
    "type": "neos-package",
    "name": "wwwision/projection-playground",
    "require": {
        "neos/neos": "*",
        "neos/contentrepository-core": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Wwwision\\ProjectionPlayground\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "Wwwision.ProjectionPlayground"
        }
    }
}
